[Unit]
Description=Investmentology Post-Screen Analysis
After=network.target postgresql.service

[Service]
Type=oneshot
WorkingDirectory=/home/investmentology
ExecStart=/usr/bin/python3 -m investmentology cron post-screen-analyze --limit 20
Environment=PYTHONPATH=/home/investmentology/src
StandardOutput=journal
StandardError=journal
TimeoutStartSec=7200
